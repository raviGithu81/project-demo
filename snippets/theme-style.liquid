{% style %}
    @font-face {
        font-family: 'Plus Jakarta Sans';
        src: url('{{ "PlusJakartaSans-Regular.woff2" | asset_url }}') format('woff2'),
            url('{{ "PlusJakartaSans-Regular.woff" | asset_url }}') format('woff');
        font-weight: normal;
        font-style: normal;
        font-display: swap;
    }
    @font-face {
        font-family: 'Plus Jakarta Sans';
        src: url('{{ "PlusJakartaSans-Medium.woff2" | asset_url }}') format('woff2'),
            url('{{ "PlusJakartaSans-Medium.woff" | asset_url }}') format('woff');
        font-weight: 500;
        font-style: normal;
        font-display: swap;
    }
    @font-face {
        font-family: 'Plus Jakarta Sans';
        src: url('{{ "PlusJakartaSans-SemiBold.woff2" | asset_url }}') format('woff2'),
            url('{{ "PlusJakartaSans-SemiBold.woff" | asset_url }}') format('woff');
        font-weight: 600;
        font-style: normal;
        font-display: swap;
    }
    @font-face {
        font-family: 'Plus Jakarta Sans';
        src: url('{{ "PlusJakartaSans-Bold.woff2" | asset_url }}') format('woff2'),
            url('{{ "PlusJakartaSans-Bold.woff" | asset_url }}') format('woff');
        font-weight: bold;
        font-style: normal;
        font-display: swap;
    }

    @font-face {
    font-family: 'icomoon';
    src: url('{{ "icomoon.eot" | asset_url }}?salyip');
    src: url('{{ "icomoon.eot" | asset_url }}?salyip#iefix') format('embedded-opentype'),
        url('{{ "icomoon.ttf" | asset_url }}?salyip') format('truetype'),
        url('{{ "icomoon.woff" | asset_url }}?salyip') format('woff'),
        url('{{ "icomoon.svg" | asset_url }}?salyip#icomoon') format('svg');
        font-weight: normal;
        font-style: normal;
        font-display: block;
    }
    :root {

        --bs-font-family-base: {{ settings.type_body_font }};
        --bs-base-font-style: {{ settings.style_body_font }};
        --bs-font-weight-base: {{ settings.type_body_font_weight }};
        --bs-font-size-base: {{ settings.type_body_font_size }};
        --bs-line-height-base: {{ settings.type_body_line_height }};
        --bs-letter-spacing-base: {{ settings.type_body_letter_spacing }};

        --bs-font-family-secondary: {{ settings.type_secondary_font }};

        --bs-font-family-heading: {{ settings.type_header_font }};
        --bs-heading-style: {{ settings.style_header_font }};
        --bs-headings-font-weight: {{ settings.type_header_font_weight }};
        --bs-line-height-heading: {{ settings.type_heading_line_height }};


        --bs-font-size-xs:{{ settings.xs_font_size }};
        --bs-font-size-sm:{{ settings.sm_font_size }};
        --bs-font-size-md:{{ settings.md_font_size }};
        --bs-font-size-lg:{{ settings.lg_font_size }};
        --bs-font-size-xl:{{ settings.xl_font_size }};

        --bs-h1-font-size:{{ settings.h1_font_size }};
        --bs-h2-font-size:{{ settings.h2_font_size }};
        --bs-h3-font-size:{{ settings.h3_font_size }};
        --bs-h4-font-size:{{ settings.h4_font_size }};
        --bs-h5-font-size:{{ settings.h5_font_size }};
        --bs-h6-font-size:{{ settings.h6_font_size }};

        --bs-h1-font-size-md:{{ settings.h1_font_size_md }};
        --bs-h2-font-size-md:{{ settings.h2_font_size_md }};
        --bs-h3-font-size-md:{{ settings.h3_font_size_md }};
        --bs-h4-font-size-md:{{ settings.h4_font_size_md }};
        --bs-h5-font-size-md:{{ settings.h5_font_size_md }};
        --bs-h6-font-size-md:{{ settings.h6_font_size_md }};

        --bs-h1-font-size-sm:{{ settings.h1_font_size_sm }};
        --bs-h2-font-size-sm:{{ settings.h2_font_size_sm }};
        --bs-h3-font-size-sm:{{ settings.h3_font_size_sm }};
        --bs-h4-font-size-sm:{{ settings.h4_font_size_sm }};
        --bs-h5-font-size-sm:{{ settings.h5_font_size_sm }};
        --bs-h6-font-size-sm:{{ settings.h6_font_size_sm }};

        --bs-font-family-button: {{ settings.type_button_font }};
        --bs-font-weight-button: {{ settings.type_button_font_weight }};
        --bs-font-size-button: {{ settings.button_font_size }};
        --bs-line-height-button: {{ settings.button_line-height }};
        --bs-letter-spacing: {{ settings.button_letter_spacing }};
        --bs-border-width-button: {{ settings.button_border-width }};
        --bs-border-radius-button: {{ settings.button_border-radius }};
        --bs-x-padding-button: {{ settings.button_x_padding }};
        --bs-y-padding-button: {{ settings.button_y_padding }};

        --bs-x-padding-button-sm: {{ settings.button_x_padding_sm }};
        --bs-y-padding-button-sm: {{ settings.button_y_padding_sm }};
        --bs-font-size-button-sm: {{ settings.button_font_size_sm }};

        --bs-x-padding-button-lg: {{ settings.button_x_padding_lg }};
        --bs-y-padding-button-lg: {{ settings.button_y_padding_lg }};
        --bs-font-size-button-lg: {{ settings.button_font_size_lg }};


        --bs-font-family-input: {{ settings.type_input_font }};
        --bs-font-weight-input: {{ settings.type_input_font_weight }};
        --bs-font-size-input: {{ settings.input_font_size }};
        --bs-line-height-input: {{ settings.input_line-height }};
        --bs-border-width-input: {{ settings.input_border-width }};
        --bs-border-radius-input: {{ settings.input_border-radius }};
        --bs-x-padding-input: {{ settings.input_x_padding }};
        --bs-y-padding-input: {{ settings.input_y_padding }};
        --bs-input-text-color: {{ settings.input_text_color }};
        --bs-input-ph-text-color: {{ settings.input_ph_text_color }};
        --bs-input-border-color: {{ settings.input_border_color }};
        --bs-input-bg-color: {{ settings.input_bg_color }}; 
        --bs-input-focus-border-color: {{ settings.input_focus_border_color }};
        --bs-input-floating-label-size:{{ settings.floating_label_size }};

        --bs-font-size-input-sm: {{ settings.input_font_size_sm }};
        --bs-x-padding-input-sm: {{ settings.input_x_padding_sm }};
        --bs-y-padding-input-sm: {{ settings.input_y_padding_sm }};

        --bs-font-size-input-lg: {{ settings.input_font_size_lg }};
        --bs-x-padding-input-lg: {{ settings.input_x_padding_lg }};
        --bs-y-padding-input-lg: {{ settings.input_y_padding_lg }};


        --bs-txt-color-btn-primary: {{ settings.txt_color_btn_primary }};
        --bs-bg-color-btn-primary: {{ settings.bg_color_btn_primary }};
        --bs-bg-hover-color-btn-primary: {{ settings.bg_hover_color_btn_primary }};

        --bs-txt-color-btn-secondary: {{ settings.txt_color_btn_secondary }};
        --bs-bg-color-btn-secondary: {{ settings.bg_color_btn_secondary }};
        --bs-bg-hover-color-btn-secondary: {{ settings.bg_hover_color_btn_secondary }};

        --bs-shadow: {{ settings.shadow }};
        --bs-shadow-sm: {{ settings.shadow-sm }};
        --bs-shadow-lg: {{ settings.shadow-lg }};


        --bs-container-width: {{ settings.container_width }};
        --bs-container-padding: {{ settings.container_padding }};


        --bs-body-bg: {{ settings.body_bg_color }};
        --bs-body-color: {{ settings.body_color }};
        --bs-heading-color: {{ settings.heading_color }};
        --bs-link-color: {{ settings.link_color }};
        --bs-link-hover-color: {{ settings.link_hover_color }};
        --bs-color-border: {{ settings.color_border }};

        --bs-color-primary: {{ settings.color_primary }};
        --bs-color-secondary: {{ settings.color_secondary }};

        --bs-color-primary-100: {{ settings.color_primary_100 }};
        --bs-color-primary-200: {{ settings.color_primary_200 }};
        --bs-color-primary-300: {{ settings.color_primary_300 }};
        --bs-color-primary-400: {{ settings.color_primary_400 }};
        --bs-color-primary-500: {{ settings.color_primary_500 }};

        --bs-color-red: {{ settings.color_red }};
        --bs-color-purple: {{ settings.color_purple }};
        --bs-color-green: {{ settings.color_green }};
        --bs-color-blue: {{ settings.color_blue }};

        --bs-color-white: {{ settings.color_white }};
        --bs-color-gray-100: {{ settings.color_gray_100 }};
        --bs-color-gray-200: {{ settings.color_gray_200 }};
        --bs-color-gray-300: {{ settings.color_gray_300 }};
        --bs-color-gray-400: {{ settings.color_gray_400 }};
        --bs-color-gray-500: {{ settings.color_gray_500 }};
        --bs-color-gray-600: {{ settings.color_gray_600 }};
        --bs-color-gray-700: {{ settings.color_gray_700 }};
        --bs-color-gray-800: {{ settings.color_gray_800 }};
        --bs-color-gray-900: {{ settings.color_gray_900 }};
        --bs-color-black: {{ settings.color_black }};


        --bs-color-success: {{ settings.color_success }};
        --bs-color-warning: {{ settings.color_warning }};
        --bs-color-danger: {{ settings.color_danger }};
        --bs-color-info: {{ settings.color_info }};

    }

    *,
    *::before,
    *::after {
        box-sizing: inherit;
    }

    html {
        box-sizing: border-box;
        font-size: 62.5%;
        height: 100%;
    }

    body {
        margin: 0;
        font-family: var(--bs-font-family-base);
        font-size: calc(var(--bs-font-size-base) * 0.94);
        font-weight: var(--bs-font-weight-base);
        line-height: calc(var(--bs-line-height-base) * 1.024);
        font-style: var(--bs-base-font-style);
        color: var(--bs-body-color);
        background-color: var(--bs-body-bg);
        letter-spacing:var(--bs-letter-spacing-base);
        -webkit-tap-highlight-color: transparent;
        -webkit-touch-callout: none;
        -webkit-user-select: none;
        -khtml-user-select: none;
        -moz-user-select: none;
        -ms-user-select: none;
        user-select: none;
    }

    @media only screen and (min-width: 992px){
        body{
        font-size: var(--bs-font-size-base);
        font-weight: var(--bs-font-weight-base);
        line-height: var(--bs-line-height-base);
        }
    }
{% endstyle %}